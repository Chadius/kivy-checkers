#:kivy 1.10.1

<TitleScreen>:
    BoxLayout:
        pos_hint: {'x':0.1, 'top':0.5}
        size_hint: 0.8, 0.5
        orientation: 'vertical'
        Button:
            text: '0'
            center_x: root.width * 1 / 4
        Button:
            text: '1'
            center_x: root.width * 2 / 4
        Button:
            text: '2'
            center_x: root.width * 3 / 4
